{
  "name": "launchy-admin",
  "version": "0.0.1",
  "description": "Launchy Admin Panel",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build NODE_OPTIONS=--max_old_space_size=4096",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "type-check": "tsc",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:status": "npx prisma migrate status",
    "migrate:resolve": "npx prisma migrate resolve",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:launchy": "npx prisma generate --schema ./prisma-launchy/schema.prisma",
    "prisma:generate:watch": "npx prisma generate --watch"
  },
  "dependencies": {
    "@adminjs/express": "^5.0.1",
    "@adminjs/nestjs": "^5.0.1",
    "@adminjs/passwords": "^3.0.1",
    "@adminjs/prisma": "^3.0.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@prisma/client": "^4.4.0",
    "accesscontrol": "^2.2.1",
    "adminjs": "^6.2.4",
    "argon2": "^0.30.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "ethers": "^5.6.9",
    "express-formidable": "^1.2.0",
    "express-http-proxy": "^1.6.3",
    "express-session": "^1.17.3",
    "ioredis": "^5.2.3",
    "joi": "^17.6.0",
    "jose": "^4.8.3",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nestjs-rollbar": "^1.7.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "remove-accents": "^0.4.3",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.3",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.11",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/express-http-proxy": "^1.6.3",
    "@types/jest": "29.0.1",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.7.16",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-universe": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.0.3",
    "prettier": "^2.3.2",
    "prettier-plugin-prisma": "^4.4.0",
    "prisma": "^4.4.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
